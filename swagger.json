{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "Carambar Jokes API",
      "description": "API for Carambar jokes"
    },
    "host": "localhost:3000",
    "basePath": "/api",
    "schemes": [
      "http"
    ],
    "paths": {
      "/jokes/random": {
        "get": {
          "summary": "Get a random joke",
          "responses": {
            "200": {
              "description": "A random joke",
              "schema": {
                "type": "object",
                "properties": {
                  "question": {
                    "type": "string"
                  },
                  "answer": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  